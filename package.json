{
  "name": "@maeum/i18n-controller",
  "version": "1.3.0",
  "description": "maeum i18n controller",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "clean": "rimraf dist",
    "dev": "tsx src/cli.ts",
    "debug": "node --inspect-brk -r ts-node/register src/cli.ts",
    "prebuild": "pnpm run clean",
    "build": "tsc --incremental --noEmit --project tsconfig.json --tsBuildInfoFile .tsbuildinfo",
    "dts": "tsc --project tsconfig.prod.json --declaration --emitDeclarationOnly --declarationDir dist/types/origin",
    "dts-gen": "rollup --config ./.configs/rollup.mjs",
    "dts:clean": "rimraf dist/types/origin",
    "esbuild-cjs": "FORMAT=cjs node .configs/esbuild.mjs",
    "esbuild-esm": "FORMAT=esm node .configs/esbuild.mjs",
    "bundle-pack": "run-p esbuild-cjs esbuild-esm dts",
    "bundle": "run-s clean bundle-pack dts-gen dts:clean",
    "test": "vitest run --coverage",
    "lint": "eslint --cache .",
    "lint-staged": "lint-staged",
    "pub": "pnpm publish --registry http://localhost:8901",
    "unpub": "pnpm unpublish typescript-cli-boilerplate@$npm_package_version --registry http://localhost:8901 --force",
    "pub:prod": "pnpm run bundle && cross-env RELEASE_MODE=true pnpm publish --access=public",
    "dd": "deps-diff md -d prod -d dev --title-list-type '*' --deps-list-type '*' --content-list-type '*'",
    "prepublishOnly": "node prepublish.cjs",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maeumjs/i18n-controller.git"
  },
  "keywords": [
    "maeum",
    "i18n",
    "i18n-controller"
  ],
  "author": "ByungJoon Lee",
  "license": "MIT",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "default": "./dist/cjs/index.cjs",
        "types": "./dist/types/index.d.ts"
      },
      "import": {
        "default": "./dist/esm/index.mjs",
        "types": "./dist/types/index.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/maeumjs/i18n-controller.git/issues"
  },
  "homepage": "https://github.com/maeumjs/i18n-controller.git#readme",
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^18.11.9",
    "@types/node-polyglot": "^2.4.2",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitest/coverage-istanbul": "^0.34.5",
    "@vitest/coverage-v8": "^0.34.5",
    "cross-env": "^7.0.3",
    "deps-diff": "^1.0.2",
    "esbuild": "0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.5.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "read-pkg": "^5.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.2",
    "rollup-plugin-dts": "^6.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.5.2",
    "typescript": "^5.2.2",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.5"
  },
  "dependencies": {
    "accept-language": "^3.0.18",
    "jsonc-parser": "^3.2.0",
    "my-easy-fp": "^0.21.0",
    "node-polyglot": "^2.5.0",
    "type-fest": "^4.3.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=production eslint --cache"
    ],
    "*.{js,jsx,ts,tsx},*.json,.{eslintrc.cjs,prettierrc}": [
      "cross-env NODE_ENV=production prettier --ignore-path .eslintignore --parser json --write"
    ]
  }
}
